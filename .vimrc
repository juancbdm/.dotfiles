syntax on

set noerrorbells
set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab
set fileformat=unix
set encoding=utf-8
set fileencoding=utf-8
set smartindent
set shiftwidth=4
set autoindent
set smartindent
set nu
set nowrap
set smartcase
set noswapfile
set nobackup
set undodir=~/.vim/undodir
set undofile
set incsearch
set clipboard=unnamedplus
set nocompatible
set number relativenumber



call plug#begin('~/.vim/plugged')

Plug 'morhetz/gruvbox'
Plug 'mbbill/undotree'

call plug#end()



